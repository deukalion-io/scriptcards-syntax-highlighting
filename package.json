{
    "name": "scriptcards",
    "displayName": "ScriptCards",
    "description": "ScriptCards for Roll20",
    "version": "0.0.1",
    "engines": {
        "vscode": "^1.67.0"
    },
    "categories": [
        "Programming Languages"
    ],
    "contributes": {
        "languages": [{
            "id": "scriptcards",
            "aliases": ["ScriptCards", "scriptcards"],
            "extensions": [".txt", ".scard"],
            "configuration": "./language-configuration.json"
        }],
        "grammars": [{
            "language": "scriptcards",
            "scopeName": "source.scard",
            "path": "./syntaxes/scriptcards.tmLanguage.json"
        }]
    }
}