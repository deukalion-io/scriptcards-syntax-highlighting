{
    "name": "scriptcards",
    "displayName": "ScriptCards Syntax Highlighting",
    "description": "Provides syntax highlighting for the ScriptCards scripting language, available on Roll20 via the ScriptCards API.",
    "version": "0.1.3",
    "publisher": "Matt Donnelly",
    "repository": "https://github.com/deukalion-io/scriptcards-syntax-highlighting",
    "license": "MIT",
    "engines": {
        "vscode": "^1.67.0"
    },
    "categories": [
        "Programming Languages"
    ],
    "keywords": [
        "scriptcards", "syntax", "highlighting", "roll20"
    ],
    "contributes": {
        "languages": [{
            "id": "scriptcards",
            "aliases": ["ScriptCards", "scriptcards"],
            "extensions": [".txt", ".scard"],
            "configuration": "./language-configuration.json"
        }],
        "grammars": [{
            "language": "scriptcards",
            "scopeName": "source.scard",
            "path": "./syntaxes/scriptcards.tmLanguage.json"
        }]
    }
}
